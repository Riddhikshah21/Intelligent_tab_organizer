{
    "manifest_version": 3,
    "name": "Intelligent Tab Organizer",
    "version": "1.0",
    "description": "Organizes your Chrome tabs intelligently",
    "permissions": [
    "tabs",
    "storage"
    ],
      "host_permissions": [
      "http://localhost:5000/"
    ],
      "browser_action": {
      "default_popup": "index.html"
    }
  }
  